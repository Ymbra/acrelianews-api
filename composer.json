{
  "name": "ymbra/acrelianews-api",
  "description": "PHP library for Acrelianews API v2",
  "type": "library",
  "keywords": ["acrelia", "acrelianews", "newsletter", "mail"],
  "license": "GPL-3.0-or-later",
  "authors": [
      {
          "name": "Nacho Salvador",
          "email": "nacho.salvador@ymbra.com"
      }
  ],
  "homepage": "https://github.com/ymbra/acrelianews-api-php",
  "require": {
    "php": "^7.4 || ^8.0",
    "guzzlehttp/guzzle": "^7.5"
  },
  "require-dev": {
      "phpunit/phpunit": "^6.0",
      "squizlabs/php_codesniffer": "^3.3.1"
  },
  "autoload": {
